{
  "editor.cursorStyle": "block",
  "files.insertFinalNewline": true,
  "window.title": "${activeEditorShort}${separator}${rootPath}",
  "files.trimTrailingWhitespace": true,
  "editor.formatOnPaste": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderControlCharacters": false,
  "editor.minimap.enabled": true,
  "editor.fontFamily": "Operator Mono Lig, Dank Mono, Fira Code, Menlo, Monaco, 'Courier New'",
  "editor.fontSize": 16,
  "editor.fontWeight": 400,
  "editor.fontLigatures": true,
  "editor.tabSize": 2,
  "editor.stickyScroll.enabled": true,
  "workbench.fontAliasing": "auto",
  "eslint.validate": ["vue", "html", "javascript", "typescript"],
  "emmet.showAbbreviationSuggestions": true,
  "emmet.showExpandedAbbreviation": "always",
  "emmet.includeLanguages": {
    "vue-html": "html",
    "vue-scss": "scss"
  },
  "explorer.confirmDelete": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "explorer.confirmDragAndDrop": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.formatOnSave": true,
  "editor.quickSuggestionsDelay": 5,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.suggest.localityBonus": true,
  "javascript.preferences.quoteStyle": "single",
  "vim.hlsearch": true,
  "vim.replaceWithRegister": true,
  "workbench.colorCustomizations": {
    "statusBar.background": "#005f5f",
    "statusBar.noFolderBackground": "#005f5f",
    "statusBar.debuggingBackground": "#005f5f"
  },
  "editor.lineNumbers": "relative",
  "vim.camelCaseMotion.enable": true,
  "diffEditor.renderSideBySide": false,
  "vim.scroll": 10,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "telemetry.telemetryLevel": "error",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "extensions.ignoreRecommendations": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editor.showTabs": "single",
  "zenMode.fullScreen": false,
  "zenMode.hideLineNumbers": false,
  "editor.lineHeight": 1.7,
  "editor.inlineSuggest.enabled": true,
  "editor.tokenColorCustomizations": {
    "[Nord]": {
      "textMateRules": [
        {
          "scope": "emphasis",
          "settings": {
            "fontStyle": "italic"
          }
        },
        {
          "scope": "strong",
          "settings": {
            "fontStyle": "bold"
          }
        },
        {
          "scope": [
            "keyword.operator.new",
            "keyword.operator.expression",
            "keyword.operator.cast",
            "keyword.operator.instanceof"
          ],
          "settings": {
            "fontStyle": "italic"
          }
        },
        {
          "name": "this.self",
          "scope": "variable.language",
          "settings": {
            "fontStyle": "italic",
            "foreground": "#ff5874"
          }
        },
        {
          "name": "@Decorator",
          "scope": ["meta.decorator punctuation.decorator"],
          "settings": {
            "fontStyle": "italic"
          }
        },
        {
          "scope": ["punctuation.definition.comment", "comment"],
          "settings": {
            "fontStyle": "italic"
          }
        },
        {
          "scope": "keyword.control",
          "settings": {
            "fontStyle": "italic"
          }
        }
      ]
    }
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "editor.accessibilitySupport": "off",
  "cSpell.userWords": ["Pressable"],
  "workbench.colorTheme": "Nord",
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true
}
